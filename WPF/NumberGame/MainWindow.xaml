<Window x:Class="NumberGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NumberGame"
        mc:Ignorable="d"
        x:Name="MainForm"
        Title="MainWindow" Height="400" Width="400" Loaded="MainForm_Loaded">
    <StackPanel>
        <Grid x:Name="grid">
            <TextBox x:Name="RessultText" HorizontalAlignment="Left" Height="30" FontSize="15" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="392" Text="TextBox"/>
        </Grid>
    </StackPanel>
</Window>
